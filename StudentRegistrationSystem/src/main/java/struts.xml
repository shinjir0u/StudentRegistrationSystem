<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="StudentRegistration" extends="struts-default">
        <action name="studentEnroll" class="student.controller.StudentAddAction" method="addStudentToHashmap">
        	<result name="success">/hashMapSuccess.jsp</result>
        </action>
        
        <action name="adminEnrollTest" class="student.controller.StudentAddAction">
        	<result name="success">/viewPages/admin/AdminEnrollPage.jsp</result>
        </action> 
        <action name="adminEnroll" class="student.controller.StudentAddAction" method="addStudentToDatabase">
        	<result name="success">/databaseSuccess.jsp</result>
        </action>
        
        <action name="adminRetrieveTest" class="student.controller.StudentRetrieveAction" method="retrieveStudent">
        	<result name="success">/viewPages/admin/AdminStudentCheckPage.jsp</result>
        </action>
    </package>
</struts>
